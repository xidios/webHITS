@using Backend4.Models.Controls
@model Backend4.Models.Controls.DropDownListViewModel
@{
    ViewBag.Title = "Drop-down List";
    var allMonths = ViewBag.AllMonths as Month[];
    var montsMap = allMonths.ToDictionary(x => x.Id);
}

<h2>Drop-down List</h2>
<dl class="dl-horizontal">
    <dt>Month</dt>
    <dd>@montsMap[Model.Month].Name</dd>
</dl>